{
  "name": "app",
  "version": "1.0.50",
  "scripts": {
    "test": "./test-setup.sh && bunx dotenv -e .env.test -- bun test",
    "test:setup": "./test-setup.sh",
    "dev": "bun run --watch src/index.ts",
    "perf:http": "bun run perf/http_load.ts",
    "db:push": "bunx dotenv -e .env.development -- prisma db push",
    "migrate:dev": "bunx dotenv -e .env.development -- prisma migrate dev",
    "generate:dev": "bunx dotenv -e .env.development -- prisma generate",
    "reset:dev": "bunx dotenv -e .env.development -- prisma migrate reset"
  },
  "dependencies": {
    "@elysiajs/jwt": "^1.4.0",
    "@prisma/adapter-mariadb": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@redis/client": "^5.10.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "elysia": "latest",
    "lodash": "^4.17.21",
    "svg-captcha": "^1.4.0",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@elysiajs/eden": "^1.4.6",
    "@types/lodash": "^4.17.23",
    "bun-types": "latest",
    "dotenv-cli": "^11.0.0",
    "prisma": "^7.3.0"
  },
  "module": "src/index.js"
}
